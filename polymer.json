{
    "builds": [{
        "preset": "es5-bundled"
    }],
    "lint": {
        "rules": [
            "polymer-2"
        ]
    },
    "entrypoint": "index.html",
    "shell": "src/cg-app.js",
    "fragments": [
        "src/cg-about.js",
        "src/cg-lessons.js",
        "src/cg-contact.js",
        "src/cg-videos.js",
        "src/cg-testimonials.js",
        "src/cg-photos.js",
        "src/cg-session.js",
        "src/cg-privacy.js",
        "src/cg-view404.js"
    ],
    "sources": [
        "src/**",
        "images/**/*",
        "bower.json"
    ],
    "extraDependencies": [
        "manifest.json",
        "data.json",
        "node_modules/webcomponentsjs/*.js"
    ]
}