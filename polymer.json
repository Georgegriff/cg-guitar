{
    "builds": [{
        "preset": "es5-bundled"
    }],
    "lint": {
        "rules": [
            "polymer-2"
        ]
    },
    "entrypoint": "index.html",
    "shell": "src/cg-app.html",
    "fragments": [
        "src/cg-about.html",
        "src/cg-lessons.html",
        "src/cg-contact.html",
        "src/cg-videos.html",
        "src/cg-testimonials.html",
        "src/cg-photos.html",
        "src/cg-session.html",
        "src/cg-privacy.html",
        "src/cg-view404.html"
    ],
    "sources": [
        "src/**",
        "images/**/*",
        "bower.json"
    ],
    "extraDependencies": [
        "manifest.json",
        "data.json",
        "bower_components/webcomponentsjs/*.js"
    ]
}