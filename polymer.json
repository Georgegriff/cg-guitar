{
    "builds": [{
        "bundle": true,
        "js": {
            "minify": true
        },
        "css": {
            "minify": true
        },
        "html": {
            "minify": true
        },
        "addServiceWorker": true
    }],
    "lint": {
        "rules": [
            "polymer-1"
        ]
    },
    "entrypoint": "index.html",
    "shell": "src/my-app.html",
    "fragments": [
        "src/my-about.html",
        "src/my-lessons.html",
        "src/my-contact.html",
        "src/my-videos.html",
        "src/my-testimonials.html",
        "src/my-photos.html",
        "src/my-session.html",
        "src/my-view404.html"
    ],
    "sources": [
        "src/**",
        "images/**/*",
        "bower.json"
    ],
    "extraDependencies": [
        "manifest.json",
        "data.json",
        "bower_components/webcomponentsjs/webcomponents-lite.min.js"
    ]
}