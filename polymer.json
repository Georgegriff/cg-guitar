{
    "builds": [{
        "preset": "es5-bundled"
    }],
    "lint": {
        "rules": [
            "polymer-2"
        ]
    },
    "entrypoint": "index.html",
    "shell": "src/my-app.html",
    "fragments": [
        "src/my-about.html",
        "src/my-lessons.html",
        "src/my-contact.html",
        "src/my-videos.html",
        "src/my-testimonials.html",
        "src/my-photos.html",
        "src/my-session.html",
        "src/my-view404.html"
    ],
    "sources": [
        "src/**",
        "images/**/*",
        "bower.json"
    ],
    "extraDependencies": [
        "manifest.json",
        "data.json",
        "bower_components/webcomponentsjs/*.js"
    ]
}